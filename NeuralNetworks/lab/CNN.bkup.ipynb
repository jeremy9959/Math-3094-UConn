    "    for i in tqdm.tqdm(range(max_iter)):\n",
    "        loss = train(model,criterion,optimizer,data, target)\n",
    "        losses.append(loss)\n",
    "        if abs(loss-prior_loss) < threshold:\n",
    "            break\n",
    "        prior_loss = loss\n",
    "            \n",
    "    return losses\n",
